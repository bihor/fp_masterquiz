{
	"name" : "fixpunkt/fp-masterquiz",
	"type" : "typo3-cms-extension",
	"description" : "A modern extension that offers quiz, polls and tests. The result can be displayed as a chart.",
	"authors" : [{
			"name" : "Kurt Gusbeth",
			"role" : "Developer"
		}
	],
	"require" : {
		"typo3/cms-core" : "~13.4.0"
	},
	"suggest": {
		"typo3/cms-dashboard": "Use widgets in dashboard"
	},
	"autoload" : {
		"psr-4" : {
			"Fixpunkt\\FpMasterquiz\\" : "Classes"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Fixpunkt\\FpMasterquiz\\Tests\\" : "Tests"
		}
	},
	"homepage" : "https://github.com/bihor/fp_masterquiz",
	"license" : "GPL-3.0-or-later",
	"keywords" : [
		"quiz poll myquizpoll"
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "fp_masterquiz"
		}
	}
}
